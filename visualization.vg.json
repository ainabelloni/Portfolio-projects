{
  "$schema": "https://vega.github.io/schema/vega/v5.json",
  "description": "Line plots that provide information regarding Covid-19 Vaccination Campaign in Italy. You can select your area of interest in the legend at the top of the visualization.",
  "background": "whitesmoke",
  "padding": 5,
  "title": {
    "text": "Covid-19 Vaccination Campaign",
    "fontSize": 40,
    "color": "#2A277C",
    "font": "Marker Felt",
    "baseline": "bottom",
    "fontWeight": "bolder",
    "anchor": "middle",
    "dy": 80
  },
  "data": [
    {
      "name": "Val_store",
      "values": [
        {
          "unit": "concat_0_layer_0",
          "fields": [{"type": "E", "field": "Areas", "hasLegend": true}],
          "values": ["Isole"]
        }
      ]
    },
    {
      "name": "hover_store",
      "values": [
        {
          "unit": "concat_0_layer_0",
          "fields": [{"type": "E", "field": "nome_area"}],
          "values": [""]
        }
      ]
    },
    {"name": "index_store"},
    {
      "name": "source_0",
      "url": "https://raw.githubusercontent.com/Francesca-2803/Covid/main/Covid_data.csv",
      "format": {
        "type": "csv",
        "parse": {"data_somministrazione": "date"},
        "delimiter": ","
      },
      "transform": [
        {
          "type": "window",
          "params": [null],
          "as": ["cumulative_count"],
          "ops": ["sum"],
          "fields": ["seconda_dose"],
          "sort": {"field": [], "order": []}
        },
        {"type": "filter", "expr": "datum.nome_area!=='NA'"},
        {"type": "filter", "expr": "datum.nome_area!==''"},
        {
          "type": "formula",
          "expr": "datum.area == 'LOM' ? 'NordOvest' : datum.area == 'LIG' ? 'NordOvest' : datum.area == 'PIE' ? 'NordOvest' : datum.area == 'VDA' ? 'NordOvest' : datum.area == 'EMR' ? 'NordEst' : datum.area == 'FVG' ? 'NordEst' : datum.area == 'PAT' ? 'NordEst' : datum.area == 'PAB' ? 'NordEst' : datum.area == 'VEN' ? 'NordEst' : datum.area == 'LAZ' ? 'Centro' : datum.area == 'MAR' ? 'Centro' : datum.area == 'TOS' ? 'Centro' : datum.area == 'UMB' ? 'Centro' : datum.area == 'ABR' ? 'Sud' :  datum.area == 'BAS' ? 'Sud' : datum.area == 'CAL' ? 'Sud' : datum.area == 'CAM' ? 'Sud' : datum.area == 'MOL' ? 'Sud' :  datum.area == 'PUG' ? 'Sud' :  datum.area == 'SAR' ? 'Isole' :  datum.area == 'SIC' ? 'Isole' :''",
          "as": "Areas"
        },
        {
          "type": "filter",
          "expr": "time(datetime(year(datum[\"data_somministrazione\"]), 0, 1, 0, 0, 0, 0))>time(datetime(2020, 0, 1, 0, 0, 0, 0))"
        }
      ]
    },
    {
      "name": "data_0",
      "source": "source_0",
      "transform": [
        {
          "type": "filter",
          "expr": "!length(data(\"Val_store\")) || vlSelectionTest(\"Val_store\", datum)"
        }
      ]
    },
    {
      "name": "data_1",
      "source": "data_0",
      "transform": [
        {
          "type": "filter",
          "expr": "isValid(datum[\"cumulative_count\"]) && isFinite(+datum[\"cumulative_count\"]) && isValid(datum[\"terapia_intensiva\"]) && isFinite(+datum[\"terapia_intensiva\"])"
        }
      ]
    },
    {
      "name": "data_2",
      "source": "source_0",
      "transform": [
        {
          "type": "formula",
          "expr": "datum.totale/datum.pop_regione",
          "as": "total2"
        },
        {
          "field": "data_somministrazione",
          "type": "timeunit",
          "units": ["week"],
          "as": ["week", "week_end"]
        }
      ]
    },
    {
      "name": "data_3",
      "source": "data_2",
      "transform": [
        {
          "type": "aggregate",
          "groupby": ["week", "Areas"],
          "ops": ["sum"],
          "fields": ["total2"],
          "as": ["sum_total2"]
        }
      ]
    },
    {
      "name": "data_4",
      "source": "data_3",
      "transform": [
        {
          "type": "filter",
          "expr": "(isDate(datum[\"week\"]) || (isValid(datum[\"week\"]) && isFinite(+datum[\"week\"]))) && isValid(datum[\"sum_total2\"]) && isFinite(+datum[\"sum_total2\"])"
        }
      ]
    },
    {
      "name": "data_5",
      "source": "data_2",
      "transform": [
        {
          "type": "filter",
          "expr": "(index.week) && (!length(data(\"index_store\")) || vlSelectionTest(\"index_store\", datum))"
        }
      ]
    },
    {
      "name": "data_6",
      "source": "data_5",
      "transform": [
        {
          "type": "aggregate",
          "groupby": ["week", "Areas"],
          "ops": ["sum"],
          "fields": ["total2"],
          "as": ["sum_total2"]
        },
        {
          "type": "filter",
          "expr": "(isDate(datum[\"week\"]) || (isValid(datum[\"week\"]) && isFinite(+datum[\"week\"]))) && isValid(datum[\"sum_total2\"]) && isFinite(+datum[\"sum_total2\"])"
        }
      ]
    },
    {
      "name": "data_7",
      "source": "data_5",
      "transform": [
        {
          "field": "data_somministrazione",
          "type": "timeunit",
          "units": ["week"],
          "as": ["week_data_somministrazione", "week_data_somministrazione_end"]
        },
        {
          "type": "filter",
          "expr": "(isDate(datum[\"week\"]) || (isValid(datum[\"week\"]) && isFinite(+datum[\"week\"])))"
        }
      ]
    }
  ],
  "signals": [
    {"name": "concat_0_width", "value": 800},
    {"name": "concat_0_height", "value": 300},
    {"name": "concat_1_width", "value": 800},
    {"name": "concat_1_height", "value": 300},
    {"name": "concat_2_width", "value": 20},
    {"name": "concat_2_height", "value": 20},
    {
      "name": "unit",
      "value": {},
      "on": [
        {"events": "mousemove", "update": "isTuple(group()) ? group() : unit"}
      ]
    },
    {
      "name": "Val_Areas_legend",
      "on": [
        {
          "events": [
            {
              "source": "view",
              "type": "click",
              "markname": "Areas_legend_symbols"
            },
            {
              "source": "view",
              "type": "click",
              "markname": "Areas_legend_labels"
            },
            {
              "source": "view",
              "type": "click",
              "markname": "Areas_legend_entries"
            }
          ],
          "update": "datum.value || item().items[0].items[0].datum.value",
          "force": true
        },
        {
          "events": [{"source": "view", "type": "click"}],
          "update": "!event.item || !datum ? null : Val_Areas_legend",
          "force": true
        }
      ]
    },
    {
      "name": "Val",
      "update": "vlSelectionResolve(\"Val_store\", \"union\", true, true)"
    },
    {
      "name": "hover",
      "update": "vlSelectionResolve(\"hover_store\", \"union\", true, true)"
    },
    {
      "name": "index",
      "update": "vlSelectionResolve(\"index_store\", \"union\", true, true)"
    }
  ],
  "layout": {"padding": 30, "columns": 1, "bounds": "full", "align": "each"},
  "marks": [
    {
      "type": "group",
      "name": "concat_0_group",
      "title": {
        "text": "People in intensive care vs Total people vaccinated with the second dose",
        "description": "This is a line chart that represents the trend of the people in intensive care corresponding to the increase in the administration of the second dose. Once you have selected an area, multiple lines are shown that correspond to the different regions of that area. Approaching the pointer of the mouse the line thickness will increase to facilitate the activation of the tooltip with the main information.",
        "fontSize": 28,
        "color": "#4851C2",
        "font": "Marker Felt",
        "fontWeight": "bolder",
        "subtitle": "through time for regions",
        "subtitleColor": "#4851C2",
        "subtitleFont": "Marker Felt",
        "subtitleFontSize": 22,
        "subtitleFontWeight": "bold",
        "anchor": "middle",
        "dy": 25,
        "offset": 50,
        "subtitlePadding": 10,
        "frame": "group"
      },
      "style": "cell",
      "encode": {
        "update": {
          "width": {"signal": "concat_0_width"},
          "height": {"signal": "concat_0_height"}
        }
      },
      "signals": [
        {
          "name": "Val_tuple",
          "update": "Val_Areas_legend !== null ? {fields: Val_tuple_fields, values: [Val_Areas_legend]} : null"
        },
        {
          "name": "Val_tuple_fields",
          "value": [{"type": "E", "field": "Areas"}]
        },
        {
          "name": "Val_toggle",
          "value": false,
          "on": [
            {
              "events": {"merge": [{"source": "view", "type": "click"}]},
              "update": "event.shiftKey"
            }
          ]
        },
        {
          "name": "Val_modify",
          "on": [
            {
              "events": {"signal": "Val_tuple"},
              "update": "modify(\"Val_store\", Val_toggle ? null : Val_tuple, Val_toggle ? null : true, Val_toggle ? Val_tuple : null)"
            }
          ]
        },
        {
          "name": "hover_tuple",
          "on": [
            {
              "events": [
                {
                  "source": "scope",
                  "type": "mouseover",
                  "markname": "concat_0_layer_0_voronoi"
                }
              ],
              "update": "datum && item().mark.marktype !== 'group' ? {unit: \"concat_0_layer_0\", fields: hover_tuple_fields, values: [(item().isVoronoi ? datum.datum : datum)[\"nome_area\"]]} : null",
              "force": true
            },
            {
              "events": [{"source": "view", "type": "dblclick"}],
              "update": "null"
            }
          ]
        },
        {
          "name": "hover_tuple_fields",
          "value": [{"type": "E", "field": "nome_area"}]
        },
        {
          "name": "hover_toggle",
          "value": false,
          "on": [
            {
              "events": [
                {
                  "source": "scope",
                  "type": "mouseover",
                  "markname": "concat_0_layer_0_voronoi"
                }
              ],
              "update": "event.shiftKey"
            },
            {
              "events": [{"source": "view", "type": "dblclick"}],
              "update": "false"
            }
          ]
        },
        {
          "name": "hover_modify",
          "on": [
            {
              "events": {"signal": "hover_tuple"},
              "update": "modify(\"hover_store\", hover_toggle ? null : hover_tuple, hover_toggle ? null : true, hover_toggle ? hover_tuple : null)"
            }
          ]
        }
      ],
      "marks": [
        {
          "name": "concat_0_layer_0_marks",
          "type": "symbol",
          "style": ["circle"],
          "interactive": true,
          "from": {"data": "data_1"},
          "encode": {
            "update": {
              "opacity": {"value": 0.7},
              "fill": {"scale": "color", "field": "Areas"},
              "ariaRoleDescription": {"value": "circle"},
              "description": {
                "signal": "\"Second Dose (cumulative): \" + (format(datum[\"cumulative_count\"], \"\")) + \"; Intensive care: \" + (format(datum[\"terapia_intensiva\"], \"\")) + \"; Select an Area: \" + (isValid(datum[\"Areas\"]) ? datum[\"Areas\"] : \"\"+datum[\"Areas\"])"
              },
              "x": {"scale": "concat_0_x", "field": "cumulative_count"},
              "y": {"scale": "concat_0_y", "field": "terapia_intensiva"},
              "size": [
                {
                  "test": "!length(data(\"hover_store\")) || vlSelectionTest(\"hover_store\", datum)",
                  "value": 7
                },
                {"value": 2}
              ],
              "shape": {"value": "circle"}
            }
          }
        },
        {
          "name": "concat_0_layer_0_voronoi",
          "type": "path",
          "interactive": true,
          "from": {"data": "concat_0_layer_0_marks"},
          "encode": {
            "update": {
              "fill": {"value": "transparent"},
              "strokeWidth": {"value": 0.35},
              "stroke": {"value": "transparent"},
              "isVoronoi": {"value": true}
            }
          },
          "transform": [
            {
              "type": "voronoi",
              "x": {"expr": "datum.datum.x || 0"},
              "y": {"expr": "datum.datum.y || 0"},
              "size": [
                {"signal": "concat_0_width"},
                {"signal": "concat_0_height"}
              ]
            }
          ]
        },
        {
          "name": "concat_0_layer_1_pathgroup",
          "type": "group",
          "from": {
            "facet": {
              "name": "faceted_path_concat_0_layer_1_main",
              "data": "data_0",
              "groupby": ["nome_area"]
            }
          },
          "encode": {
            "update": {
              "width": {"field": {"group": "width"}},
              "height": {"field": {"group": "height"}}
            }
          },
          "marks": [
            {
              "name": "concat_0_layer_1_marks",
              "type": "line",
              "style": ["line"],
              "sort": {"field": "datum[\"cumulative_count\"]"},
              "interactive": true,
              "from": {"data": "faceted_path_concat_0_layer_1_main"},
              "encode": {
                "update": {
                  "stroke": [
                    {"test": "datum.area == 'ABR'", "value": "#6C2761"},
                    {"test": "datum.area == 'BAS'", "value": "#8C317E"},
                    {"test": "datum.area == 'VDA'", "value": "#C5C1EB"},
                    {"test": "datum.area == 'PIE'", "value": "#9992DB"},
                    {"test": "datum.area == 'EMR'", "value": "#16193F"},
                    {"test": "datum.area == 'SAR'", "value": "#3A9CA4"},
                    {"test": "datum.area == 'CAM'", "value": "#C44EB2"},
                    {"test": "datum.area == 'UMB'", "value": "#A0E0B3"},
                    {"test": "datum.area == 'LAZ'", "value": " #338E4E"},
                    {"test": "datum.area == 'LOM'", "value": "#6D64C9"},
                    {"test": "datum.area == 'FVG'", "value": "#232865"},
                    {"test": "datum.area == 'SIC'", "value": "#87D1D7"},
                    {"test": "datum.area == 'PAT'", "value": "#6EBFCF"},
                    {"test": "datum.area == 'TOS'", "value": "#71D08D"},
                    {"test": "datum.area == 'PAB'", "value": "#2F368C"},
                    {"test": "datum.area == 'VEN'", "value": "#5860CA"},
                    {"test": "datum.area == 'PUG'", "value": "#DB8DCE"},
                    {"test": "datum.area == 'MOL'", "value": "#CF6DC0"},
                    {"test": "datum.area == 'CAL'", "value": "#AD3B9B"},
                    {"test": "datum.area == 'LIG'", "value": "#483EAF"},
                    {"test": "datum.area == 'MAR'", "value": "#43BE68"},
                    {"test": "datum.Areas == 'Sud'", "value": "#CF6BC9"},
                    {"test": "datum.Areas == 'NordOvest'", "value": "#9B5FCD"},
                    {"test": "datum.Areas == 'NordEst'", "value": "#4C7FC4"},
                    {"test": "datum.Areas == 'Isole'", "value": "#53BCC9"},
                    {"test": "datum.Areas == 'Centro'", "value": "#61CB75"},
                    {"scale": "color", "field": "nome_area"}
                  ],
                  "tooltip": {
                    "signal": "{\"Region\": isValid(datum[\"nome_area\"]) ? datum[\"nome_area\"] : \"\"+datum[\"nome_area\"], \"Date\": timeFormat(datum[\"data_somministrazione\"], '%b %d, %Y'), \"Intensive care\": format(datum[\"terapia_intensiva\"], \"\")}"
                  },
                  "description": {
                    "signal": "\"Second Dose (cumulative): \" + (format(datum[\"cumulative_count\"], \"\")) + \"; Intensive care: \" + (format(datum[\"terapia_intensiva\"], \"\")) + \"; Select an Area: \" + (isValid(datum[\"nome_area\"]) ? datum[\"nome_area\"] : \"\"+datum[\"nome_area\"]) + \"; Region: \" + (isValid(datum[\"nome_area\"]) ? datum[\"nome_area\"] : \"\"+datum[\"nome_area\"]) + \"; Date: \" + (timeFormat(datum[\"data_somministrazione\"], '%b %d, %Y'))"
                  },
                  "x": {"scale": "concat_0_x", "field": "cumulative_count"},
                  "y": {"scale": "concat_0_y", "field": "terapia_intensiva"},
                  "strokeWidth": [
                    {
                      "test": "!length(data(\"hover_store\")) || vlSelectionTest(\"hover_store\", datum)",
                      "value": 7
                    },
                    {"value": 2}
                  ],
                  "defined": {
                    "signal": "isValid(datum[\"cumulative_count\"]) && isFinite(+datum[\"cumulative_count\"]) && isValid(datum[\"terapia_intensiva\"]) && isFinite(+datum[\"terapia_intensiva\"])"
                  }
                }
              }
            }
          ]
        }
      ],
      "axes": [
        {
          "scale": "concat_0_x",
          "orient": "bottom",
          "gridScale": "concat_0_y",
          "grid": true,
          "tickCount": {"signal": "ceil(concat_0_width/40)"},
          "domain": false,
          "labels": false,
          "aria": false,
          "maxExtent": 0,
          "minExtent": 0,
          "ticks": false,
          "zindex": 0
        },
        {
          "scale": "concat_0_y",
          "orient": "left",
          "gridScale": "concat_0_x",
          "grid": true,
          "tickCount": {"signal": "ceil(concat_0_height/40)"},
          "domain": false,
          "labels": false,
          "aria": false,
          "maxExtent": 0,
          "minExtent": 0,
          "ticks": false,
          "zindex": 0
        },
        {
          "scale": "concat_0_x",
          "orient": "bottom",
          "grid": false,
          "title": "Second Dose (cumulative)",
          "labelAlign": "left",
          "labelBaseline": "top",
          "labelFlush": true,
          "labelOverlap": true,
          "tickCount": {"signal": "ceil(concat_0_width/40)"},
          "zindex": 0
        },
        {
          "scale": "concat_0_y",
          "orient": "left",
          "grid": false,
          "title": "Intensive care",
          "labelAlign": "right",
          "labelOverlap": true,
          "tickCount": {"signal": "ceil(concat_0_height/40)"},
          "zindex": 0
        }
      ]
    },
    {
      "type": "group",
      "name": "concat_1_group",
      "title": {
        "text": "Vaccine doses administrated by region",
        "fontSize": 30,
        "anchor": "middle",
        "color": "#4851C2",
        "font": "Marker Felt",
        "baseline": "bottom",
        "fontWeight": "bolder",
        "subtitle": "normalized for the population from December 2020 to September 2021 by week",
        "subtitleColor": "#4851C2",
        "subtitleFont": "Marker Felt",
        "subtitleFontSize": 22,
        "subtitleFontWeight": "bold",
        "frame": "group"
      },
      "style": "cell",
      "encode": {
        "update": {
          "width": {"signal": "concat_1_width"},
          "height": {"signal": "concat_1_height"},
          "description": {
            "value": "The second graph is a multi series line chart that illustrates the trend of the total vaccine doses administrated each week divided by population from December 25th, 2020 to September 16th, 2021. The colored line corresponds to the selected area. Moving the pointer of the mouse towards the graph, a vertical line appears that indicates the date."
          }
        }
      },
      "signals": [
        {
          "name": "index_tuple",
          "on": [
            {
              "events": [
                {
                  "source": "scope",
                  "type": "mousemove",
                  "markname": "concat_1_layer_1_voronoi"
                }
              ],
              "update": "datum && item().mark.marktype !== 'group' ? {unit: \"concat_1_layer_1\", fields: index_tuple_fields, values: [(item().isVoronoi ? datum.datum : datum)[\"week\"]]} : null",
              "force": true
            },
            {
              "events": [{"source": "view", "type": "dblclick"}],
              "update": "null"
            }
          ]
        },
        {
          "name": "index_tuple_fields",
          "value": [{"field": "week", "channel": "x", "type": "E"}]
        },
        {
          "name": "index_toggle",
          "value": false,
          "on": [
            {
              "events": [
                {
                  "source": "scope",
                  "type": "mousemove",
                  "markname": "concat_1_layer_1_voronoi"
                }
              ],
              "update": "event.shiftKey"
            },
            {
              "events": [{"source": "view", "type": "dblclick"}],
              "update": "false"
            }
          ]
        },
        {
          "name": "index_modify",
          "on": [
            {
              "events": {"signal": "index_tuple"},
              "update": "modify(\"index_store\", index_toggle ? null : index_tuple, index_toggle ? null : true, index_toggle ? index_tuple : null)"
            }
          ]
        }
      ],
      "marks": [
        {
          "name": "concat_1_layer_0_pathgroup",
          "type": "group",
          "from": {
            "facet": {
              "name": "faceted_path_concat_1_layer_0_main",
              "data": "data_3",
              "groupby": ["Areas"]
            }
          },
          "encode": {
            "update": {
              "width": {"field": {"group": "width"}},
              "height": {"field": {"group": "height"}}
            }
          },
          "marks": [
            {
              "name": "concat_1_layer_0_marks",
              "type": "line",
              "style": ["line"],
              "sort": {"field": "datum[\"week\"]"},
              "interactive": false,
              "from": {"data": "faceted_path_concat_1_layer_0_main"},
              "encode": {
                "update": {
                  "stroke": [
                    {
                      "test": "!length(data(\"Val_store\")) || vlSelectionTest(\"Val_store\", datum)",
                      "scale": "color",
                      "field": "Areas"
                    },
                    {"value": "grey"}
                  ],
                  "opacity": [
                    {
                      "test": "!length(data(\"Val_store\")) || vlSelectionTest(\"Val_store\", datum)",
                      "value": 1
                    },
                    {"value": 0.2}
                  ],
                  "description": {
                    "signal": "\"Administration Date: \" + (timeFormat(datum[\"week\"], '%b %d, %Y')) + \"; Vaccine doses administered divided by population: \" + (format(datum[\"sum_total2\"], \"\")) + \"; Select an Area: \" + (isValid(datum[\"Areas\"]) ? datum[\"Areas\"] : \"\"+datum[\"Areas\"])"
                  },
                  "x": {"scale": "concat_1_x", "field": "week"},
                  "y": {"scale": "concat_1_y", "field": "sum_total2"},
                  "strokeWidth": {"value": 3},
                  "defined": {
                    "signal": "isValid(datum[\"week\"]) && isFinite(+datum[\"week\"]) && isValid(datum[\"sum_total2\"]) && isFinite(+datum[\"sum_total2\"])"
                  }
                }
              }
            }
          ]
        },
        {
          "name": "concat_1_layer_1_marks",
          "type": "symbol",
          "style": ["point"],
          "interactive": true,
          "from": {"data": "data_4"},
          "encode": {
            "update": {
              "fill": {"value": "transparent"},
